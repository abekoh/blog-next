{
  "version": 2,
  "functions": {
    "pages/api/ogp-images/[postid]/index.ts": {
      "includeFiles": "assets/**"
    }
  },
  "public": false,
  "github": {
    "enabled": false
  },
  "builds": [
    { "src": "./public/**", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "public/$1" }
  ]
}
