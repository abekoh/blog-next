{
  "name": "blog-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "now-build": "scripts/update_info.sh && yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid,libz}.so.1 node_modules/canvas/build/Release/ && yarn build",
    "build": "next build",
    "start": "next start",
    "lint": "eslint 'src/**/*.{ts,tsx}' && prettier --check '**/*.{js,ts,tsx,json}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}' && prettier --write '**/*.{js,ts,tsx,json}'",
    "typecheck": "tsc --pretty --noEmit",
    "test": "jest && playwright test",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:ci": "jest --ci",
    "api:build": "aspida"
  },
  "dependencies": {
    "@aspida/fetch": "^1.11.0",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.1",
    "@mui/styles": "^5.11.1",
    "@octokit/rest": "^19.0.5",
    "@types/moment": "^2.13.0",
    "@types/prismjs": "^1.26.0",
    "canvas": "^2.11.0",
    "jsonld": "^8.1.0",
    "moment": "^2.29.4",
    "next": "^13.1.1",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-share": "^4.4.1",
    "rss": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@octokit/types": "^8.0.0",
    "@playwright/test": "^1.29.1",
    "@types/google.analytics": "^0.0.42",
    "@types/jest": "^29.2.4",
    "@types/jsonld": "^1.5.8",
    "@types/marked": "^4.0.8",
    "@types/material-ui": "^0.21.12",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/rss": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-prismjs": "^2.1.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "marked": "^4.2.5",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
