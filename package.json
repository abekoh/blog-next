{
  "name": "blog-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "now-build": "scripts/update_info.sh && yum install libuuid-devel libmount-devel zlib && cp /lib64/{libuuid,libmount,libblkid,libz}.so.1 node_modules/canvas/build/Release/ && yarn build",
    "build": "next build",
    "start": "next start",
    "lint": "eslint 'src/**/*.{ts,tsx}' && prettier --check '**/*.{js,ts,tsx,json}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}' && prettier --write '**/*.{js,ts,tsx,json}'",
    "typecheck": "tsc --pretty --noEmit",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "api:build": "aspida",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@aspida/fetch": "^1.6.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@material-ui/core": "^5.0.0-alpha.28",
    "@material-ui/icons": "^4.11.2",
    "@types/moment": "^2.13.0",
    "@types/prismjs": "^1.16.5",
    "canvas": "^2.7.0",
    "jsonld": "^5.2.0",
    "moment": "^2.29.1",
    "next": "10.0.9",
    "prismjs": "^1.23.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "rss": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.2.9",
    "@types/google.analytics": "^0.0.41",
    "@types/jest": "^26.0.21",
    "@types/jsonld": "^1.5.5",
    "@types/material-ui": "^0.21.8",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/rss": "^0.0.28",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-prismjs": "^2.0.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "marked": "^2.0.1",
    "prettier": "^2.2.1",
    "storybook-addon-material-ui": "^0.9.0-alpha.24",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
