{
  "name": "blog-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "now-build": "scripts/update_info.sh && yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid,libz}.so.1 node_modules/canvas/build/Release/ && yarn build",
    "build": "next build",
    "start": "next start",
    "lint": "eslint 'src/**/*.{ts,tsx}' && prettier --check '**/*.{js,ts,tsx,json}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}' && prettier --write '**/*.{js,ts,tsx,json}'",
    "typecheck": "tsc --pretty --noEmit",
    "test": "jest && playwright test",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:ci": "jest --ci",
    "api:build": "aspida"
  },
  "dependencies": {
    "@aspida/fetch": "^1.7.1",
    "@emotion/cache": "^11.6.0",
    "@emotion/react": "^11.6.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.1.1",
    "@mui/material": "^5.1.1",
    "@mui/styles": "^5.1.1",
    "@octokit/rest": "^18.12.0",
    "@types/moment": "^2.13.0",
    "@types/prismjs": "^1.16.6",
    "canvas": "2.6.1",
    "jsonld": "^5.2.0",
    "moment": "^2.29.4",
    "next": "12.1.0",
    "prismjs": "^1.27.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-share": "^4.4.0",
    "rss": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@octokit/types": "^6.34.0",
    "@playwright/test": "^1.18.1",
    "@types/google.analytics": "^0.0.42",
    "@types/jest": "^27.0.3",
    "@types/jsonld": "^1.5.6",
    "@types/marked": "^4.0.1",
    "@types/material-ui": "^0.21.12",
    "@types/node": "^16.11.9",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/rss": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-prismjs": "^2.1.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.3.1",
    "marked": "^4.0.10",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
